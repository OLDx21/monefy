<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.monefy"


>

    <application
            android:allowBackup="true"
            android:icon="@drawable/monefy"
            android:label="@string/app_name"
            android:roundIcon="@drawable/monefy"
            android:supportsRtl="true"
            android:theme="@style/Theme.Monefy">

        <activity


                android:name=".activitys.MinusActivity">

        </activity>
        <activity

                android:screenOrientation="portrait"
                android:name=".activitys.CostMinus">

        </activity>
        <activity
                android:screenOrientation="portrait"
                android:name=".activitys.PlusActivity">
        </activity>

        <activity
                android:screenOrientation="portrait"
                android:name=".activitys.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/Theme.Monefy.AppBarOverlay"
               >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>